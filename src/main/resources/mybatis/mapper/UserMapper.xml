<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.bcu.ls.mapper.UserMapper">

	
	<insert id="insertUser"
		parameterType="cn.eud.bcu.ls.entity.User">
		insert into user
		(user_name,user_pwd,user_school,user_integral,user_phonenumber,user_type,user_schoolnumber,class_id,dormitory_id) values
		(#{user_name},#{user_pwd},#{user_school},#{user_integral},#{user_phonenumber},#{user_type},#{user_schoolnumber},#{class_id},#{dormitory_id})
	</insert>


</mapper>